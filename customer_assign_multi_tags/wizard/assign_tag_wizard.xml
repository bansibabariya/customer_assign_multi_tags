<odoo>

    <record id="model_assign_tags_form" model="ir.ui.view">
        <field name="name">assign.tags.form</field>
        <field name="model">assign.tags</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="partner_ids" widget="many2many_tags" invisible="1"/>
                    <field name="tag_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button name="action_assign_tags" string="Assign Tags" type="object" default_focus="1" class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>